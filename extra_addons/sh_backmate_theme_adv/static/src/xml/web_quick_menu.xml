                    <span class="fa fa-bookmark sh_bookmarked_icon" t-on-click="open_quick_menu"
                          t-on-dblclick="on_click"></span>
                </a>
            </div>
            <div class="dropdown-menu dropdown-menu-right sh_wqm_quick_menu_submenu_list_cls"
                 aria-labelledby="dropdownMenuButton"></div>
        </div>
        </t>
    </t>

    <t t-name="quick.menulist.actions">
        <t t-call="quick.menulist.search"/>
        <div class="sh_search_result">
            <t t-call="quick.menulist"/>
        </div>

    </t>
    <t t-name="quick.menulist.search">
